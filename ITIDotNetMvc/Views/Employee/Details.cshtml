@model EmployeeDepartmentViewModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<h2 style="color:@Model.Color">@Model.EmployeeName</h2>

<h2>@Model.DepartmentName</h2>
<h2>@Model.Msg</h2>

<select class="form form-control">
    @foreach (var item in Model.Branches)
    {
        <option>@item</option>
    }
</select>
